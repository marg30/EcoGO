        <servlet-name>DesafioServlet</servlet-name>
        <url-pattern>/desafios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DesafioCompletadoServlet</servlet-name>
        <url-pattern>/gracias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DatosUsuarioServlet</servlet-name>
        <url-pattern>/miusuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
